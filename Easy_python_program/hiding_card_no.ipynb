     "text": [
      "************5678\n",
      "************3213\n",
      "**********5523\n"
     ]
    }
   ],
   "source": [
    "def card_hide(card):\n",
    "    str = ''\n",
    "    for char in card[:-4]:\n",
    "        str+='*'\n",
    "    str+=card[-4:]\n",
    "    return str\n",
    "\n",
    "\n",
    "print(card_hide(\"1234123456785678\"))\n",
    "print(card_hide(\"8754456321113213\")) \n",
    "print(card_hide(\"35123413355523\") )"
   ]

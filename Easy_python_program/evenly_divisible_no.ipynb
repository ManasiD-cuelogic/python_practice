   "source": [
    "def evenly_divisible(a, b, c):\n",
    "    sum1 = 0\n",
    "    for i in range(a,b):\n",
    "        if i % c == 0:\n",
    "            sum1+=i\n",
    "    print(sum1)\n",
    "    \n",
    "evenly_divisible(1, 10, 20)\n",
    "evenly_divisible(1, 10, 2)\n",
    "evenly_divisible(1, 10, 3) "

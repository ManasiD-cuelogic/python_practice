     "output_type": "stream",
     "text": [
      "Enter number: 234\n",
      "3\n"
     ]
    }
   ],
   "source": [
    "def number_length(num):\n",
    "    count = 0\n",
    "    while num != 0:\n",
    "        num = num // 10\n",
    "        count+=1\n",
    "        \n",
    "    print(count)\n",
    "    \n",
    "    \n",
    "num = int(input(\"Enter number: \"))\n",
    "number_length(num)"
   ]
